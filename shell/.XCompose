# Mantém o comportamento padrão das dead keys
<dead_acute> <space>      : "'"
<dead_grave> <space>      : "`"
<dead_tilde> <space>      : "~"
<dead_circumflex> <space> : "^"
<dead_diaeresis> <space>  : "\""

# Acentos agudos (´)
<dead_acute> <a>     : "á"
<dead_acute> <e>     : "é"
<dead_acute> <i>     : "í"
<dead_acute> <o>     : "ó"
<dead_acute> <u>     : "ú"
<dead_acute> <A>     : "Á"
<dead_acute> <E>     : "É"
<dead_acute> <I>     : "Í"
<dead_acute> <O>     : "Ó"
<dead_acute> <U>     : "Ú"

# Cedilha
<dead_acute> <c>     : "ç"
<dead_acute> <C>     : "Ç"

# Til (~)
<dead_tilde> <a>     : "ã"
<dead_tilde> <o>     : "õ"
<dead_tilde> <A>     : "Ã"
<dead_tilde> <O>     : "Õ"

# Crase (`)
<dead_grave> <a>     : "à"
<dead_grave> <A>     : "À"

# Circunflexo (^)
<dead_circumflex> <a>     : "â"
<dead_circumflex> <e>     : "ê"
<dead_circumflex> <o>     : "ô"
<dead_circumflex> <A>     : "Â"
<dead_circumflex> <E>     : "Ê"
<dead_circumflex> <O>     : "Ô"

# Trema (¨)
<dead_diaeresis> <a>      : "ä"
<dead_diaeresis> <e>      : "ë"
<dead_diaeresis> <i>      : "ï"
<dead_diaeresis> <o>      : "ö"
<dead_diaeresis> <u>      : "ü"
<dead_diaeresis> <A>      : "Ä"
<dead_diaeresis> <E>      : "Ë"
<dead_diaeresis> <I>      : "Ï"
<dead_diaeresis> <O>      : "Ö"
<dead_diaeresis> <U>      : "Ü"
